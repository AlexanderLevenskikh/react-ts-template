{
  "name": "react-ts-template",
  "version": "1.0.0",
  "description": "Template for react-typescript projects",
  "main": "index.js",
  "scripts": {
    "build:dev": "cross-env TS_NODE_PROJECT=config/webpack/tsconfig.json webpack --config config/webpack/config.ts --env.mode=development",
    "build:prod": "cross-env TS_NODE_PROJECT=config/webpack/tsconfig.json webpack --config config/webpack/config.ts --env.mode=production",
    "start:dev": "cross-env TS_NODE_PROJECT=config/webpack/tsconfig.json webpack-dev-server --config config/webpack/config.ts --env.mode=development --env.wds",
    "start:prod": "cross-env TS_NODE_PROJECT=config/webpack/tsconfig.json webpack-dev-server --config config/webpack/config.ts --env.mode=production --env.wds",
    "test:unit": "npx jest",
    "test:unit:update": "npx jest ---updateSnapshot",
    "test:unit:debug": "node %NODE_DEBUG_OPTION% node_modules/jest/bin/jest.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexanderLevenskikh/react-ts-template.git"
  },
  "author": "Alexander Levenskikh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AlexanderLevenskikh/react-ts-template/issues"
  },
  "homepage": "https://github.com/AlexanderLevenskikh/react-ts-template#readme",
  "dependencies": {
    "antd": "^3.26.12",
    "classnames": "^2.2.6",
    "core-js": "^3.6.4",
    "qs": "^6.9.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "router5": "^7.0.2",
    "uuid": "^7.0.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@hot-loader/react-dom": "^16.11.0",
    "@types/classnames": "^2.2.9",
    "@types/html-webpack-plugin": "^3.2.2",
    "@types/jest": "^25.1.3",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/node": "^13.7.4",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.1",
    "@types/qs": "^6.9.1",
    "@types/react": "^16.9.22",
    "@types/react-dom": "^16.9.5",
    "@types/terser-webpack-plugin": "^2.2.0",
    "@types/uuid": "^3.4.7",
    "@types/webpack": "^4.41.6",
    "@types/webpack-dev-server": "^3.10.0",
    "@types/webpack-env": "^1.15.1",
    "autoprefixer": "^9.7.4",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "file-loader": "^5.1.0",
    "fork-ts-checker-webpack-plugin": "^4.0.4",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-flexbugs-fixes": "^4.2.0",
    "postcss-loader": "^3.0.0",
    "react-hot-loader": "^4.12.19",
    "regenerator-runtime": "^0.13.3",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.5",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.2",
    "webpack": "^4.41.6",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
